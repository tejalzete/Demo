<script>     
      $(document).ready(function(){
    	 function validateForm(e){
    	 $("div").remove(".alert-danger");
	     var isValid = true;
	     $('input[type="text"]').each(function() {
	     if ($.trim($(this).val()) == '') {
	         isValid = false;
	         $(this).css({"border": "1px solid red"});
	         $(this).after("<div class=\"alert-danger\" >Field can not be empty.</div>");
	     }
	     else {
	         $(this).css({"border": "","background": ""});
	     }
	     });
	     $('input[type="textarea"]').each(function() {
		     if ($.trim($(this).val()) == '') {
		         isValid = false;
		         $(this).css({"border": "1px solid red"});
		     }else {
		         $(this).css({"border": "","background": ""});
		     }
	     });     
		     return isValid;
   }
		function callService(url,requstParams,body){
			$('.spinner-loading-overlay').show();
			$.ajax({
	            type: "POST",
	            url: url,
	            contentType: 'application/json',
	            dataType: 'json',
	            data: JSON.stringify(body),
	            timeout : 100000,
	            success: function( data ) {
	              this1.find('input[type=submit]').attr('disabled', false);
	              $('.spinner-loading-overlay').hide();
	              if(data.fatalError){
	            	  this.showError(data.message);  
	              }else{
	            	  this.showSuccess(data.message);  
	              }
	            },
	            error: function(jqXHR, exception, errorThrown) {
	                this1.find('input[type=submit]').attr('disabled', false);
	                $('.spinner-loading-overlay').hide();

	            },
	            complete: function() {
	                this1.find('input[type=submit]').attr('disabled', false);
	                $('.spinner-loading-overlay').hide();
	                
	            },showError:function(message){
	                 		if(message.name!=undefined)
	                 			this.errorTest("#commentName",message.name);
	                 		if(message.email!=undefined)
	                 			this.errorTest("#commentEmail",message.email);
	                  		if(message.comments!=undefined)
	                  			this.errorTest("#commentText",message.comments);
	                  		if(message.mobile!=undefined)
	                  			this.errorTest("#commentMobile",message.mobile);
	                  		if(message.commentssuccess!=undefined)
	                  			this.errorTest("#commentssuccess",message.commentssuccess);
	            },showSuccess:function(message){
	            	$('#commentsForm').trigger("reset");
	            	this1.find("#commentssuccess").append("<div class=\"alert-success\">"+message.commentssuccess+"</div>");	
	            },errorTest:function(key,value){
	    			this1.find(key).after("<div class=\"alert-danger\" >"+value+"</div>");
	    		}
	            
			});
						
		}
        // event on submit either login, register or forgot form.
        $("#commentsForm").submit( function(e) {
          e.preventDefault();
          this1 = $(this);
          $('.spinner-loading-overlay').show();
          this1.find(".extra").empty();
          this1.find('input[type=submit]').attr('disabled', true);
          if(validateForm(e)){
        	if(e.target.id==="commentsForm"){
       		var request={};
          	var url;
        	url= 'http://localhost:8089/Demo/addComment';
        	request.email=$("#commentEmail").val();
        	request.name=$("#commentName").val();
        	request.mobile=$("#commentMobile").val();
        	request.comments=$("#commentText").val();
          }
          callService(url,"",request)
          }
      });
      });
    </script>
